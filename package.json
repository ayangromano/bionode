  },
  "devDependencies": {
    "all-contributors-cli": "^4.3.0",
    "async": "^2.3.0",
    "contributor": "^0.1.25",
    "coveralls": "~2.13.0",
    "debug": "^2.6.4",
    "dependency-check": "^2.8.0",
    "docco": "~0.7.0",
    "doxx": "^2.1.1",
    "glob": "^7.1.1",
    "istanbul": "~0.4.5",
    "jsdom": "^10.0.0",
    "nock": "^9.0.13",
    "pumpify": "^1.3.5",
    "request": "^2.81.0",
